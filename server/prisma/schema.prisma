generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Form {
  id       String @id @unique @default(uuid())
  fullName String
  phone    String @unique
  email    String @unique
  password String
  profile  String
}

model File {
  id         String @id @unique @default(uuid())
  filename   String
  secure_url String
  bytes      Int
  format     String
}
